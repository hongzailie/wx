/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff');
    font-weight: normal;
    font-style: normal;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-1:before {
  content: "\e623";
}

.icon-7:before {
  content: "\e624";
}

.icon-10:before {
  content: "\e625";
}

.icon-8:before {
  content: "\e626";
}

.icon-17:before {
  content: "\e627";
}
#biao{
  width: 100%;
  position: fixed;
  bottom: 0;
  display: flex;
  justify-content: space-around;
  height: 60px;
}
.shang{
  height: 30px;
  width: 30px;
}
.iconfont{
  line-height: 30px;
  margin: 0 auto;
}
.zi{
  font-size: 14px;
  line-height: 30px;
}
.zi:hover{
  color: blue;
}

